    "storySource": {
      "source": "() => {\n  const weight = 'strong';\n\n  const appearances = [\n    'primary',\n    'secondary',\n    'alert',\n    'warning',\n    'success',\n    'accent1',\n    'accent2',\n    'accent3',\n    'accent4',\n  ];\n\n  return (\n    <div className=\"d-flex\">\n      {appearances.map((appear, ind) => {\n        return (\n          <div key={ind} className=\"mr-9\">\n            <Text weight={weight}>{appear.charAt(0).toUpperCase() + appear.slice(1)}</Text>\n            <br />\n            <br />\n            <Avatar firstName=\"John\" lastName=\"Doe\" appearance={appear} />\n          </div>\n        );\n      })}\n    </div>\n  );\n}"
    },
