      "source": "() => {\n  const schema = {\n    name: 'name',\n    displayName: 'Name',\n  };\n\n  const size = 'comfortable';\n\n  const width = 250;\n\n  const cellType = 'STATUS_HINT';\n\n  const align = 'left';\n\n  const tooltip = true;\n\n  const loading = false;\n\n  return (\n    <div className=\"Grid-cell Grid-cell--body\" style={{ width, border: 'var(--border)' }}>\n      <div className=\"Grid-cellContent\">\n        <GridCell\n          size={size}\n          rowIndex={1}\n          colIndex={1}\n          loading={loading}\n          data={{\n            name: {\n              title: 'Schreiber Brynn',\n              metaList: ['Meta Item 1', 'Meta Item 2'],\n              icon: 'events',\n            },\n          }}\n          schema={{\n            ...schema,\n            width,\n            cellType,\n            align,\n            tooltip,\n          }}\n        />\n      </div>\n    </div>\n  );\n}"
    },
